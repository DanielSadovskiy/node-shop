include head

div(class='wrap')
  div(class='content')
    header(class='jumbotron')
      include  header

    div(class='container')
      div(class='row')
        each val2, index2 in cat
            div(class="col col-md-12")
                h2
                    a(
                        href='/category?id='+val2['id']+"&page=1"
                    )   #{val2['category']}
            
            each val, index in goods
                if val2['id'] == val['category']
                    include card

    include nav
    include footer